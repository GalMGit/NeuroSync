{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "JwtOptions": {
    "SecretKey": "TmV1dHJvbk5ldCBTZWNyZXRLZXkgMjAyNCBKV1QgU2VjdXJpdHkgQXV0aA==",
    "ExpiresHours": "10"
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "auth-all": {
        "ClusterId": "auth-cluster",
        "Match": {
          "Path": "/api/auth/{action}"
        },
        "AuthorizationPolicy": "PublicAccess"
      },
      "user-all": {
        "ClusterId": "user-cluster",
        "Match": {
          "Path": "/api/user/{action}"
        },
        "AuthorizationPolicy": "PublicAccess"
      },
      "user-profile": {
        "ClusterId": "user-cluster",
        "Match": {
          "Path": "/api/user/profile"
        },
        "AuthorizationPolicy": "Authenticated"
      }
    },
    "Clusters": {
      "auth-cluster": {
        "Destinations": {
          "auth-service": {
            "Address": "http://localhost:5235"
          }
        }
      },
      "user-cluster": {
        "Destinations": {
          "user-service": {
            "Address": "http://localhost:5296"
          }
        }
      }
    }
  }
}